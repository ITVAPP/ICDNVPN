{
  "log": {
    "loglevel": "info"
  },
  "stats": {},
  "policy": {
    "levels": {
      "8": {
        "handshake": 4,
        "connIdle": 300,
        "uplinkOnly": 1,
        "downlinkOnly": 1
      }
    },
    "system": {
      "statsOutboundUplink": true,
      "statsOutboundDownlink": true
    }
  },
  "api": {
    "tag": "api",
    "services": [
      "StatsService"
    ]
  },
  "fakedns": [
    {
      "ipPool": "198.18.0.0/15",
      "poolSize": 65535
    },
    {
      "ipPool": "fc00::/18",
      "poolSize": 65535
    }
  ],
  "dns": {
    "hosts": {
      "dns.google": "8.8.8.8",
      "dns.alidns.com": ["223.5.5.5", "223.6.6.6"],
      "doh.pub": ["1.12.12.12", "120.53.53.53"],
      "dot.pub": ["1.12.12.12", "120.53.53.53"],
      "dns.cloudflare.com": ["1.1.1.1", "1.0.0.1"],
      "cloudflare-dns.com": ["1.1.1.1", "1.0.0.1"],
      "one.one.one.one": ["1.1.1.1", "1.0.0.1"],
      "dns.quad9.net": "9.9.9.9",
      "proxy.example.com": "127.0.0.1"
    },
    "servers": [
      {
        "address": "fakedns",
        "domains": [
          "geosite:geolocation-!cn"
        ]
      },
      "1.1.1.1",
      "8.8.8.8",
      {
        "address": "223.5.5.5",
        "port": 53,
        "domains": [
          "geosite:cn",
          "geosite:geolocation-cn"
        ],
        "expectIPs": [
          "geoip:cn"
        ],
        "skipFallback": true
      }
    ]
  },
  "inbounds": [
    {
      "tag": "socks",
      "port": 7898,
      "protocol": "socks",
      "settings": {
        "auth": "noauth",
        "udp": true,
        "userLevel": 8
      },
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls", "fakedns"],
        "metadataOnly": false,
        "routeOnly": false
      }
    },
    {
      "tag": "http",
      "port": 7899,
      "protocol": "http",
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls", "fakedns"],
        "metadataOnly": false,
        "routeOnly": false
      },
      "settings": {
        "auth": "noauth",
        "udp": true,
        "allowTransparent": false,
        "userLevel": 8
      }
    },
    {
      "tag": "api",
      "port": 10085,
      "protocol": "dokodemo-door",
      "settings": {
        "address": "127.0.0.1"
      }
    },
    {
      "tag": "dns-in",
      "port": 53,
      "listen": "198.18.0.2",
      "protocol": "dokodemo-door",
      "settings": {
        "address": "198.18.0.2",
        "port": 53,
        "network": "tcp,udp"
      }
    }
  ],
  "outbounds": [
    {
      "tag": "proxy",
      "protocol": "vless",
      "settings": {
        "vnext": [
          {
            "address": "SERVER_IP",
            "port": 443,
            "users": [
              {
                "id": "bc24baea-3e5c-4107-a231-416cf00504fe",
                "alterId": 0,
                "email": "t@t.tt",
                "security": "auto",
                "encryption": "none",
                "level": 8
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "security": "tls",
        "tlsSettings": {
          "allowInsecure": true,
          "serverName": "pages-vless-a9f.pages.dev",
          "fingerprint": "randomized"
        },
        "wsSettings": {
          "path": "/",
          "headers": {
            "Host": "pages-vless-a9f.pages.dev"
          }
        },
        "sockopt": {
          "dialerProxy": "proxy3"
        }
      },
      "mux": {
        "enabled": false,
        "concurrency": -1
      }
    },
    {
      "tag": "direct",
      "protocol": "freedom",
      "settings": {}
    },
    {
      "tag": "block",
      "protocol": "blackhole",
      "settings": {
        "response": {
          "type": "http"
        }
      }
    },
    {
      "tag": "dns-out",
      "protocol": "dns",
      "settings": {}
    },
    {
      "tag": "proxy3",
      "protocol": "freedom",
      "settings": {
        "fragment": {
          "packets": "tlshello",
          "length": "100-200",
          "interval": "10-20"
        }
      }
    }
  ],
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "inboundTag": ["api"],
        "outboundTag": "api"
      },
      {
        "type": "field",
        "inboundTag": ["dns-in"],
        "outboundTag": "dns-out"
      },
      {
        "type": "field",
        "ip": [
          "198.18.0.0/15"
        ],
        "outboundTag": "proxy"
      },
      {
        "type": "field",
        "port": "53",
        "ip": [
          "223.5.5.5",
          "223.6.6.6",
          "1.12.12.12",
          "120.53.53.53",
          "119.29.29.29",
          "114.114.114.114",
          "114.114.115.115",
          "180.76.76.76",
          "180.184.1.1",
          "180.184.2.2"
        ],
        "outboundTag": "direct"
      },
      {
        "type": "field",
        "port": "53",
        "ip": [
          "1.1.1.1",
          "1.0.0.1",
          "8.8.8.8",
          "8.8.4.4",
          "9.9.9.9"
        ],
        "outboundTag": "proxy"
      },
      {
        "type": "field",
        "port": "443",
        "network": "udp",
        "outboundTag": "block"
      },
      {
        "type": "field",
        "ip": [
          "geoip:private"
        ],
        "outboundTag": "direct"
      },
      {
        "type": "field",
        "outboundTag": "direct",
        "domain": [
          "domain:dns.alidns.com",
          "domain:doh.pub",
          "domain:dot.pub",
          "domain:doh.360.cn",
          "domain:dot.360.cn",
          "geosite:cn",
          "geosite:geolocation-cn"
